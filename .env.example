# Assemble Electron App Environment Variables

# Node Environment: 'production', 'development', or 'local'
NODE_ENV=development

# Production Firebase Configuration
FIREBASE_SERVICE_ACCOUNT_TYPE=service_account
FIREBASE_PROJECT_ID=your-production-project-id
FIREBASE_PRIVATE_KEY_ID=your-production-private-key-id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour Production Private Key Here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your-production-client-email@example.com
FIREBASE_CLIENT_ID=your-production-client-id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-production-service-account
FIREBASE_VAPID_KEY=your-production-vapid-key

# Development Firebase Configuration
DEV_FIREBASE_SERVICE_ACCOUNT_TYPE=service_account
DEV_FIREBASE_PROJECT_ID=your-development-project-id
DEV_FIREBASE_PRIVATE_KEY_ID=your-development-private-key-id
DEV_FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour Development Private Key Here\n-----END PRIVATE KEY-----\n"
DEV_FIREBASE_CLIENT_EMAIL=your-development-client-email@example.com
DEV_FIREBASE_CLIENT_ID=your-development-client-id
DEV_FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
DEV_FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
DEV_FIREBASE_AUTH_PROVIDER_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
DEV_FIREBASE_CLIENT_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-development-service-account
DEV_FIREBASE_VAPID_KEY=your-development-vapid-key

# Other app settings
# Add any other environment variables your app needs here
